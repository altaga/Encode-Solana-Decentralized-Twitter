cidl: "0.9"
info:
  name: xSol
  title: xSol
  version: 0.0.1
  license:
    name: Unlicense
    identifier: Unlicense
types:
  Tweet:
    fields:
      - name: text
        type: string
        attributes: [ cap=64 ]
      - name: creator
        type: sol:pubkey
methods:
  - name: addTweet
    inputs:
      - name: tweet_account
        type: sol:account<Tweet>
        attributes: [ sol:writable, sol:init_if_needed]
